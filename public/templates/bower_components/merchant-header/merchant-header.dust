{!
	@param showNotifications		Boolean indicating whether to show or hide utility nav
	@param showSettings				Boolean indicating whether to show or hide feedback link
!}

<header>
	<div class="merchantHeader">
		<div class="contentWrapper">
			<a href="#contents" class="skip">{@pre type="content" key="merchant-header.skipToMain"/}</a>

			<a href="/" class="logo">
				<img alt="{@pre type="content" key="merchant-header.logoIconAlt" editable="false" /}" src="images/src/asna.gif" height=50>
			</a>

			<nav role="navigation" id="navMenu">
				<ul class="primaryNav">
					<li class="homeIcon hidden-sm" data-explore="{@pre type="content" key="merchant-header.helpHome" editable="false" /}">
						<a href="{@pre type="content" key="merchant-header.home.link" editable="false" /}" class="{@if cond="'{viewName}' == 'summary/index'"}selected{/if}">
							<span class="accessAid">{@pre type="content" key="merchant-header.homeIconAlt"  /}</span>
						</a>
					</li>
					<li>
						<a href="{@pre type="content" key="merchant-header.money.link" editable="false" /}" class="{@if cond="'{viewName}' == 'money/index'"}selected{/if}">
							{@pre type="content" key="merchant-header.money.content"  /}
						</a>
					</li>
					<li>
						<a href="{@pre type="content" key="merchant-header.travel.link" editable="false" /}" class="{@if cond="'{viewName}' == 'travel'"}selected{/if}">
							{@pre type="content" key="merchant-header.travel.content"  /}
						</a>
					</li>
					<li>
						<a href="{@pre type="content" key="merchant-header.cc.link" editable="false" /}" class="{@if cond="'{viewName}' == 'cc'"}selected{/if}">
							{@pre type="content" key="merchant-header.cc.content"  /}
						</a>
					</li>
					<li class="hidden-sm">
						<a href="{@pre type="content" key="merchant-header.shopping.link" editable="false" /}" class="{@if cond="'{viewName}' == 'shopping'"}selected{/if}">{@pre type="content" key="merchant-header.shopping.content"  /}</a>
					</li>
					<li class="moreMenu">
						<a href="#" class="moreLink">{@pre type="content" key="merchant-header.more"  /}</a>
						<ul>
							<li class="visible-sm">
								<a href="{@pre type="content" key="merchant-header.tools.link" editable="false" /}" class="{@if cond="'{viewName}' == 'merchant/tools'"}selected{/if}">{@pre type="content" key="merchant-header.tools.content"  /}</a>
							</li>
							<li>
								<a href="{@pre type="content" key="merchant-header.reports.link" editable="false" /}">{@pre type="content" key="merchant-header.reports.content"  /}</a>
							</li>
							<li>
								<a href="{@pre type="content" key="merchant-header.resCenter.link" editable="false" /}">{@pre type="content" key="merchant-header.resCenter.content"  /}</a>
								<span class="divider"></span>
							</li>
							<li id='siteFeedback'>
								{>"bower_components/merchant-opinion-lab/merchant-opinion-lab"
									commentCardCmd=""
									showOpinionLabOnBrowserClose="false"
									sfRedirect="false"
									showSiteFeedbackIcon="false"
									miniBrowser="false"
									siteFeedbackLabelKey="{@pre type="content" key="merchant-header.feedback" editable="false" /}" /}
							</li>
							<li>
								<a href="{@pre type="content" key="merchant-header.whatsNew.link" editable="false" /}">{@pre type="content" key="merchant-header.whatsNew.content"/}</a>
							</li>
							<li>
								<a href="{@pre type="content" key="merchant-header.ccReport.link" editable="false" /}">{@pre type="content" key="merchant-header.ccReport.content"/}</a>
							</li>
							<li>
								<a href="{@pre type="content" key="merchant-header.rentReport.link" editable="false" /}">{@pre type="content" key="merchant-header.rentReport.content"/}</a>
							</li>
							{!TODO - hide this link for falcon onboard accounts !}
							{^whiteList.hideSwitchToClassic}
								<li>
									<span class="divider"></span>
								<a href="{@pre type="content" key="merchant-header.switch.link" editable="false" /}">{@pre type="content" key="merchant-header.switch.content"  /}</a>
								</li>
							{/whiteList.hideSwitchToClassic}
							<li class="visible-xs">
								<a href="{@pre type="content" key="merchant-header.logout.link" editable="false" /}">{@pre type="content" key="merchant-header.logout.content"  /}</a>
							</li>
						</ul>
					</li>
				</ul>
			</nav>

			<div class="globalLinksHolder">
				{! notifications flyout !}
				{@if cond="'{showNotifications}' == 'true'"}
					{>"bower_components/merchant-header/notifications"/}
				{/if}
				{! settings flyout !}
				{@if cond="'{showSettings}' == 'true'"}
					{>"bower_components/merchant-header/settings-flyout"/}
				{/if}

				<div class="logoutWrapper hidden-xs">
					<a class="button secondary small" href="{@pre type="content" key="merchant-header.logout.link" editable="false" /}">
						{@pre type="content" key="merchant-header.logout.content"  /}
					</a>
				</div>
				<button id="hamburger-button"></button>	
			</div>
		</div>
	</div>
</header>

